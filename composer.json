{
    "name": "base-php/base-php",
    "description": "Un framework para minimalistas",
    "type": "project",
    "license": "MIT",
    "version": "1.5.0",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Nisa Delgado",
            "email": "nisadelgado@gmail.com"
        }
    ],
    "require": {
        "base-php/core": "1.5.0",
        "illuminate/database": "9.42.2",
        "illuminate/pagination": "9.42.2",
        "illuminate/routing": "9.42.2",
        "illuminate/validation": "9.42.2",
        "illuminate/view": "9.42.2",
        "league/flysystem": "3.10.4",
        "phpmailer/phpmailer": "6.6.5"
    },
    "require-dev": {
        "fakerphp/faker": "1.20.0",
        "phpunit/phpunit": "9.5.26",
        "psy/psysh": "0.11.9",
        "spatie/ignition": "1.4.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "vendor/base-php/core/bootstrap/autoload.php"
        ]
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "base-php/core",
                "version": "1.5.0",
                "dist": {
                    "url": "https://github.com/base-php/core/archive/refs/heads/main.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "scripts": {
        "pre-install-cmd": [
            "composer clear-cache"
        ],
        "post-install-cmd": [
            "composer dump-autoload --optimize"
        ]
    }
}
