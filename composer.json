{
    "name": "nisadelgado/base",
    "description": "Estructura base para proyectos PHP",
    "type": "project",
    "license": "MIT",
    "version": "1.3.17",
    "authors": [
        {
            "name": "Nisa Delgado",
            "email": "nisadelgado@gmail.com"
        }
    ],
    "require": {
        "filp/whoops": "2.14.0",
        "illuminate/database": "8.52.0",
        "illuminate/events": "8.52.0",
        "illuminate/pagination": "8.52.0",
        "illuminate/routing": "8.52.0",
        "illuminate/view": "8.52.0",
        "league/flysystem": "2.2.0",
        "nisadelgado/framework": "1.3.17",
        "phpmailer/phpmailer": "6.5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "vendor/nisadelgado/framework/autoload.php"
        ]
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "nisadelgado/framework",
                "version": "1.3.17",
                "dist": {
                    "url": "https://github.com/nisa6delgado/framework/archive/refs/heads/main.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "scripts": {
        "pre-install-cmd": [
            "composer clear-cache"
        ],
        "post-install-cmd": [
            "composer dump-autoload --optimize"
        ]
    }
}
